{
        "AWSEBDockerrunVersion": 1,
        "containerDefintions" : [
            {
                "name": "client",
                "image": "ashboro/udemymulti-client",
                "hostname": "client",
                "essential": false,
                "memory": 128
        
            }
            ,
            {
                "name": "server",
                "image": "ashboro/udemymulti-server",
                "hostname": "api",
                "essential": false,
                "memory": 128
        
            }
            ,
            {
                "name": "worker",
                "image": "ashboro/udemymulti-worker",
                "hostname": "worker",
                "essential": false,
                "memory": 128
        
            }
            ,
            {
                "name": "nginx",
                "image": "ashboro/udemymulti-nginx",
                "essential": true,
                "portMappings": [
                    {
                        "hostPort": 80,
                        "containerPort": 80
                    }
                ],
                "links": ["client","server" ],
                "memory": 128
        
            }
        ]

}