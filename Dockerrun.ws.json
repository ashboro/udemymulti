{

        "AWSEBDockerrunVersion" : 2,
        "containerDefintions" : [
            {
                "name": "client",
                "image": "ashboro/multi-client",
                "hostname": "client",
                "essential": false
        
            }
            ,
            {
                "name": "server",
                "image": "ashboro/multi-server",
                "hostname": "api",
                "essential": false
        
            }
            ,
            {
                "name": "worker",
                "image": "ashboro/multi-worker",
                "hostname": "worker",
                "essential": false
        
            }
            ,
            {
                "name": "nginx",
                "image": "ashboro/multi-nginx",
                "essential": true,
                "portMappings": [
                    {
                        "hostPort": 80,
                        "containerPort": 80
                    }
                ],
                "links":[
                    "client","server"
                ]
        
            }
        ]

}